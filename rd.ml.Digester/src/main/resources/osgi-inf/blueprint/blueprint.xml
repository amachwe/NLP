<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">
	<bean id = "digester" class = "rd.ml.digester.service.DigestServiceImpl"></bean>
	<service ref = "digester" interface = "rd.ml.digester.service.DigestService"/>
	
	<bean id="mongosink" class="rd.ml.digester.service.MongoDataSink">
		<argument value="mongodb://192.168.0.17:27017"/>
		<argument value="27017"/>
		<argument value="NLP"/>
		<argument value="documents"/>
		<argument value="topics"/>
		
	</bean>
	<service ref="mongosink" interface="rd.ml.digester.service.DataSink"/>
</blueprint>